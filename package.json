{
  "name": "@fantasies/cdk",
  "version": "0.0.12",
  "description": "utils",
  "author": "devcui <devcui@outlook.com>",
  "license": "MIT",
  "homepage": "https://github.com/devcui/fantasies-cdk#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/devcui/fantasies-cdk.git"
  },
  "bugs": {
    "url": "https://github.com/devcui/fantasies-cdk/issues"
  },
  "keywords": [
    "fantasies",
    "cdk",
    "vue",
    "typescript",
    "vite"
  ],
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "LICENSE",
    "README.md",
    "dist/*"
  ],
  ".": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "npm run type:check && npm run build:only",
    "build:only": "vite build",
    "watch": "vite build --watch",
    "type:check": "vue-tsc -p tsconfig.check.json --noEmit",
    "type:gen": "vue-tsc --declaration  --emitDeclarationOnly",
    "test": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:update": "vitest -u",
    "lint": "eslint . --ext .ts,.vue",
    "lint:fix": "eslint . --ext .ts,.vue --fix"
  },
  "peerDependencies": {
    "vue": "^3.5.17"
  },
  "dependencies": {
    "@vue/runtime-core": "^3.5.17",
    "vue": "^3.5.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.16.2",
    "@types/node": "^24.0.13",
    "@unocss/eslint-plugin": "^66.3.3",
    "@vitejs/plugin-vue": "^6.0.0",
    "@vitest/browser": "^3.2.4",
    "@vitest/coverage-istanbul": "^3.2.4",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.31.0",
    "eslint-plugin-format": "^1.0.1",
    "jsdom": "^26.1.0",
    "msw": "^2.10.4",
    "node": "^24.4.0",
    "playwright": "^1.54.1",
    "typescript": "^5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4",
    "vitest-axe": "^0.1.0",
    "vitest-browser-vue": "^1.0.0",
    "vitest-canvas-mock": "^0.3.3",
    "vitest-package-exports": "^0.1.1",
    "vue-tsc": "^3.0.1"
  }
}
